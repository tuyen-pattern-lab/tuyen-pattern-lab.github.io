{{#each checkboxes }}
<div class="form-check">
  <input class="form-check-input" type="checkbox" value="{{value}}" id="{{id}}" {{#if checked}}checked{{/ if}} {{#if disabled }}disabled{{/ if }} {{#if readOnly }}readonly{{/ if }}>
  <label class="form-check-label" for="{{id}}">
    {{label}}
  </label>
</div>
{{/each}}